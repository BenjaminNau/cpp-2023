/*
Write a program that accepts as input the mass, in grams, and density, in grams per cubic centimeters, and outputs the volume of the object using the formula: volume = mass / density.

Format your output to two decimal places.
*/

#include <iostream>
#include <iomanip>

using namespace std;

int main() {
    double massInGrams, densityInGramsPerCubicCentimeter;

    cout << "what is the mass, in grams, and density, in grams per cubic centimeters ";
    cin >> massInGrams >> densityInGramsPerCubicCentimeter;

    double volume = massInGrams / densityInGramsPerCubicCentimeter;

    cout << fixed << setprecision(2);
    cout << "the volume of the object using the formula: volume = mass / density is " << volume << endl;
    return 0;
}
